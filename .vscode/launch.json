{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run & Debug React App (Chrome)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/src",
            "sourceMaps": true,
            "preLaunchTask": "npm: start",
            "timeout": 30000
        }
    ]
}